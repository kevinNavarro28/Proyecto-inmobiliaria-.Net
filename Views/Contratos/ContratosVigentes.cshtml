@model IEnumerable<Inmobiliaria.Models.Contratos>

<h1>Contratos de Alquiler Vigentes</h1>
<table class="table">
    <thead>
        <tr>
            <th>NÃºmero de Contrato</th>
            <th>Fecha Inicio</th>
            <th>Fecha Fin</th>
            <th>Monto</th>
            <th>Inmueble</th>
            <th>Inquilino</th>
        </tr>
    </thead>
    <tbody>
    @foreach (var contrato in Model)
    {
        <tr>
            <td>@contrato.Id</td>
            <td>@contrato.Fecha_Inicio.ToString("dd/MM/yyyy")</td>
            <td>@contrato.Fecha_Fin.ToString("dd/MM/yyyy")</td>
            <td>$@contrato.Monto</td>
            <td>@contrato.InmuebleId</td>
            <td>@contrato.InquilinoId</td>
        </tr>
    }
    </tbody>
</table>
<div>
    <a asp-action="IndexC"><i class="fa-solid fa-left-long fa-2xl" style="color: #005eff;"></i></a>
</div>